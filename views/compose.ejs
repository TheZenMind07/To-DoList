<%- include("partials/header"); -%>
  <h1>Compose</h1>
  <form action="/compose" method="POST">

    <div class="form-group">
      <label for="composeTitle">Title</label>
      <input type="text" class="form-control" id="composeTitle" placeholder="Enter Title">
    </div>
    <div class="form-group">
      <label for="composeText">Content</label>
      <textarea class="form-control" id="composeText" rows="3"></textarea>
    </div>
    <button type="submit" class="btn btn-primary" name="button">Publish</button>

  </form>
  <%- include("partials/footer"); -%>